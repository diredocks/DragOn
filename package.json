    "postinstall": "wxt prepare"
  },
  "dependencies": {
    "@webext-core/messaging": "^2.3.0",
    "solid-js": "^1.9.10"
  },
  "devDependencies": {
    "@wxt-dev/module-solid": "^1.1.4",
    "typescript": "^5.9.3",
    "wxt": "^0.20.6"
  }
}
